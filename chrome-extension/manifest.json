{
  "name": "Testool",
  "description": "Companion app for QA",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "dist/background.js"
  },
  "action": {
    "default_title": "execute.js will run (watch the Chrome DevTools' console)"
  },
  "permissions": ["tabs", "scripting", "activeTab"],
  "host_permissions": ["https://*/*", "http://*/*"]
}
